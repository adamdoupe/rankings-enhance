{
    "name": "Rankings Enhance",
    "description": "Enhance csrankings.org with additional features.",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://csrankings.org/*"],
            "js": ["content-script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["rankings-enhance.js"],
            "matches": ["*://csrankings.org/*"]
        }
    ]
}
